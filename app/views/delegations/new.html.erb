<%= dm_form_for @delegation, title: 'Nuova delega' do |d| %>  
  <%= d.dm_error_notification %>
  <%= d.input :delegator_upn, input_html: { class: "choosen_dsauser" } %>
  <%= d.input :delegate_upn,  input_html: { class: "choosen_dsauser" } %>
  <%= d.submit %>  
<% end %>

<%= javascript_tag do  %>  
  <%= render partial: "dsausers/awesomplete", formats: :js, locals: { input_id: 'delegation_delegator_upn', input2_id: 'delegation_delegate_upn' } %>  
<% end %>
